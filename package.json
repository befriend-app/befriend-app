      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      }
    }
  },
  "dependencies": {
    "axios": "^1.7.7",
    "chokidar": "^3.6.0",
    "csso": "^5.0.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "gm": "^1.25.0",
    "sass": "^1.78.0",
    "terser": "^5.32.0",
    "yargs": "^17.7.2"
  }
