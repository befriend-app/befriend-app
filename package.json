{
  "name": "app.befriend",
  "displayName": "Befriend",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "contributors": [
    {
      "name": "Eugene Gekhter",
      "email": "eugene@befriend.app"
    },
    {
      "name": "Logan Phillips",
      "email": "logan@befriend.app"
    }
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
